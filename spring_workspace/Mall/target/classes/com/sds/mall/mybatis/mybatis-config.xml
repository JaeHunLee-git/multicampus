	<mappers>
		<mapper resource="com/sds/mall/mybatis/TopCategoryMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/SubCategoryMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/ProductMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/ColorMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/PsizeMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/MemberMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/MemberDetailMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/SnSMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/CartMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/ReceiverMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/PaymethodMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/OrderSummaryMapper.xml" />
		<mapper resource="com/sds/mall/mybatis/OrderDetailMapper.xml" />
	</mappers>
	
</configuration>